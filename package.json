{
  "name": "dcopy",
  "version": "1.0.0",
  "description": "",
  "files": [
    "dist",
    "sync"
  ],
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./sync": {
      "require": "./sync/index.js",
      "import": "./sync/index.mjs",
      "types": "./sync/index.d.ts"
    }
  },
  "scripts": {
    "build-async": "tsup-node src/index.ts --out-dir=dist --format cjs,esm",
    "build-sync": "tsup-node src/sync/* --out-dir=sync --format cjs,esm",
    "build-fast": "yarn build-async && yarn build-sync",
    "build": "rm -rf dist && rm -rf sync && yarn run build-async -- --dts-resolve && yarn run build-sync -- --dts-resolve",
    "test": "uvu -r esbuild-register test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^17.0.5",
    "@types/tmp": "^0.2.3",
    "esbuild-register": "^3.3.1",
    "mk-dirs": "^3.0.0",
    "tmp": "^0.2.1",
    "tsup": "^5.11.9",
    "typescript": "^4.5.4",
    "uvu": "^0.5.2"
  }
}
